<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.paper.hdwallpaperapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/wallpaper_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/wallpaper_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.HDWallpaperApp">
        <activity android:name="com.paper.hdwallpaperapp.MainActivity"/>
        <activity android:name="com.paper.hdwallpaperapp.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>